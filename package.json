{
  "name": "portfolio",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy:production": "yarn -n \"-r dotenv/config\" && GATSBY_AWS_S3_BUCKET_NAME=jamesabaja.com GATSBY_ENV=production yarn build && firebase use production && firebase deploy --only hosting:jamesabaja-815ef && firebase use default",
    "deploy:staging": "yarn -n \"-r dotenv/config\" && GATSBY_ENV=staging yarn build && gatsby-plugin-s3 deploy"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.27.2",
    "bulma": "^0.8.0",
    "classnames": "^2.2.6",
    "formik": "^2.2.9",
    "gatsby": "^2.19.7",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sass": "^2.1.30",
    "moment": "^2.29.3",
    "node-sass": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
